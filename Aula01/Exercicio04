#include <stdio.h>
#include <stdlib.h>

int main()
{
float comprimento, largura, altura, volume;
printf("Comprimento: ");
scanf("%f", &comprimento);
printf("Largura: ");
scanf("%f", &largura);
printf("Altura: ");
scanf("%f", &altura);

volume = comprimento * largura * altura;
printf("A caixa com \nComprimento: %.2f \nLargura: %.2f \nAltura: %.2f \nVolume Total: %.2f", comprimento, largura, altura, volume);
return 0;
}
