    #include <stdio.h>
    #include <stdlib.h>
    #include <math.h>

    int main()
    {
        //Aula 02 Exercicio 02
        float R, PI;
        PI = 3.1415;
        printf("Digite R:");
        scanf("%f", &R);

        printf("Raio: %.2f \nVolume:%.3f", R, (4*PI*pow(R,3))/3);

        return 0;
    }
