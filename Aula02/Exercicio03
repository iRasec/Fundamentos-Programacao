    #include <stdio.h>
    #include <stdlib.h>
    #include <math.h>

    int main()
    {
       char nome[30];
       float salariobr, salarioliq;
       printf("Nome do Funcionario: ");
       gets(nome);
       printf("Salario Bruto: ");
       scanf("%f", &salariobr);
       salarioliq = salariobr * ((100-20.5)/100);

       printf("O salario Liquido do %s apos descontos: %.2f", nome, salarioliq);

    }
