    #include <stdio.h>
    #include <stdlib.h>
    #include <math.h>



    int main()
    {
    //Exercicio 06 da Aula 02
    int reais,centavos, notas_cem, notas_cinquenta, notas_vinte, notas_dez, notas_cinco, resto, moedas_um, moedas_cinquenta, moedas_vintecinco, moedas_dez, moedas_cinco, notas_dois;
    scanf("%d,%d", &reais, &centavos);
    //Reais
    notas_cem = reais/100;
    resto = reais % 100;

    notas_cinquenta = resto / 50;
    resto = resto % 50;

    notas_vinte = resto / 20;
    resto = resto % 20;

    notas_dez = resto / 10;
    resto = resto % 10;

    notas_cinco = resto / 5;
    resto = resto % 5;

    notas_dois = resto / 2;
    resto = resto % 2;

    moedas_um = resto / 1;
    //Centavos
    moedas_cinquenta = centavos / 100;
    resto = centavos % 100;
    moedas_cinquenta = resto / 50;
    resto = resto % 50;
    moedas_vintecinco = resto / 25;
    resto = resto % 25;
    moedas_dez = resto /10;
    resto = resto % 10;
    moedas_cinco = resto / 5;
    resto = resto % 5;

    printf("\nNotas Cem: %d", notas_cem);
    printf("\nNotas Cinquenta: %d", notas_cinquenta);
    printf("\nNotas Vinte: %d", notas_vinte);
    printf("\nNotas Dez: %d ", notas_dez);
    printf("\nNotas Cinco: %d", notas_cinco);
    printf("\nNotas Dois: %d", notas_dois);
    printf("\nMoedas 1: %d", moedas_um);
    printf("\nMoedas 50: %d", moedas_cinquenta);
    printf("\nMoedas 25: %d", moedas_vintecinco);
    printf("\nMoedas 10: %d", moedas_dez);
    printf("\nMoedas 5: %d", moedas_cinco);



    return 0;
    }
