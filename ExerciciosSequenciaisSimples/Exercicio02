    #include <stdio.h>
    #include <stdlib.h>
    #include <math.h>



    int main()
    {
    float comprimento, largura, altura, volume;

    printf("Digite o comprimento: ");
    scanf("%f", &comprimento);

    printf("Digite a largura: ");
    scanf("%f", &largura);

    printf("Digite a altura:  ");
    scanf("%f", &altura);

    volume = comprimento * largura * altura;

    printf("Volume de: %.2f", volume);
    return 0;
    }
